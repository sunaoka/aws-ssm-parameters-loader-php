{
    "name": "sunaoka/aws-ssm-parameters-loader-php",
    "description": "Load values from AWS SSM Parameter store into environment variables",
    "keywords": ["aws", "ssm", "env"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sunaoka",
            "email": "sunaoka@pocari.org"
        }
    ],
    "require": {
        "php": "^8.0",
        "aws/aws-sdk-php": "^3.20.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12.0",
        "phpunit/phpunit": "^9.6 || ^10.5 || ^11.2"
    },
    "autoload": {
        "psr-4": {
            "Sunaoka\\SsmParametersLoader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sunaoka\\SsmParametersLoader\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
